<configuration>
	<appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%date{yyyy-MM-dd HH:mm:ss.SSS} %a "%r" %s %responseHeader{Content-Length} "%i{Referer}" "%i{User-Agent}" %D "%i{X-Forwarded-For}" %v
			</pattern>
		</encoder>
	</appender>

	<appender-ref ref="STDOUT"/>
</configuration>